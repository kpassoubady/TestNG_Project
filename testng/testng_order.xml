<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Order Suite" parallel="classes" thread-count="2">
    <test name="Order Test Cases">
        <groups>
            <run>
                <include name="fruit"/>
                <exclude name="banana"/>
            </run>
        </groups>
        <classes>
            <class name="com.kavinschool.testng.examples.OrderExecutionTest"/>
        </classes>
    </test> <!-- TestNgExamples -->
</suite> <!-- Default Suite -->
